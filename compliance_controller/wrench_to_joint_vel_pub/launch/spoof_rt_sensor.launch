<launch>
  <node name="ra_spoof_ft_sensor" pkg="wrench_to_joint_vel_pub" type="spoof_ft_sensor" output="screen" >
    <rosparam command="load" file="$(find wrench_to_joint_vel_pub)/config/compliance_settings.yaml" />

    <param name="joy_topic" value="/joy"/>
    <param name="z_offset" value="-1.0"/>
    <param name="z_scale" value="-5"/>

  </node>
  <node name="joy" pkg="joy" type="joy_node">
    <param name="deadzone" value="0.1"/>
    <param name="autorepeat_rate" value="10"/>
  </node>
</launch>
